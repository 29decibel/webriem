=form_for [@doc_head,@vouch] do |f|
  = f.hidden_field :id
  -%w(dbill_date idoc cbill ccode cdept_id s_cdept_id cperson_id s_cperson_id citem_id ccode_equal).each do |col|
    = tb_input_field(f,col)
  .actions
    =f.submit '保存',:class=>'btn primary'
    =link_to "返回",doc_head_vouches_path(@doc_head),:class=>'btn'


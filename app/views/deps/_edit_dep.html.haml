-form_for dep,:remote=>true,:class=>"edit_inline" do |f|
  = render "shared/common_error",:resource=>dep
  =f.hidden_field :id
  .form_field
    %span.label= f.label "#{I18n.t('common_attr.code')}#{mark_required(f.object,:code)}"
    %span.value= f.text_field :code,:class=>"input1 " 
  .form_field
    %span.label= f.label "#{I18n.t('common_attr.name')}#{mark_required(f.object,:name)}" 
    %span.value= f.text_field :name,:class=>"input1 "
  .form_field
    %span.label= f.label "#{I18n.t('activerecord.attributes.dep.version')}#{mark_required(f.object,:version)}" 			
    %span.value= f.text_field :version,:class=>"input1" 
  .form_field
    %span.label= f.label "#{I18n.t('activerecord.attributes.dep.start_date')}#{mark_required(f.object,:start_date)}" 
    %span.value= f.text_field :start_date,:class=>"datepicker input1" 
  .form_field
    %span.label= f.label "#{I18n.t('activerecord.attributes.dep.end_date')}#{mark_required(f.object,:end_date)}" 			
    %span.value= f.text_field :end_date,:class=>"datepicker input1" 
  .form_field
    %span.label= f.label "#{I18n.t('activerecord.attributes.dep.u8dep_code')}#{mark_required(f.object,:u8dep_code)}" 
    %span.value= ts "u8_dep_id","U8Dep",:text=>f.object.u8_dep.to_s,:value=>f.object.u8_dep_id,:f=>f
  .form_field
    %span.label= f.label "#{I18n.t('activerecord.attributes.dep.parent_dep_id')}" 			
    %span.value= ts "parent_dep_id","Dep",:text=>f.object.parent_dep.to_s,:value=>f.object.parent_dep_id,:f=>f
  =render "basic_setting/fb_actions"

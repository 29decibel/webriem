//render the partial
<%- model_s_name=ActiveModel::Naming.singular(resource) %>
$("#edit_<%= "#{model_s_name}_#{resource.id}"%>").html("<%= escape_javascript(render "edit_#{model_s_name}",model_s_name.to_sym=>resource)%>");
//show it
$("#edit_<%= "#{model_s_name}_#{resource.id}"%>").show("slow");

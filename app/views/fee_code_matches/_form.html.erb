<%= form_for(@fee_code_match) do |f| %>
<h3 class="form_title"><%= @title%></h3>
<div class="field">
	<%= f.label :fee_id,:class=>"input_label" %>
	<div class="input_area">
		<p class="input">
    <div class="reference">
			<%= f.hidden_field :fee_id,{:class=>"ref_hidden_field"}%>
			<%= text_field_tag "fee_info",@fee_code_match.fee.to_s,:class=>"ref" %>
			<a href="#" class-data="Fee"><span class="reference"></span></a>			
		</div>

		</p>
		<p class="tip"></p>
	</div>
	<span class="error_message"></span>    
  </div>
<div class="field">
	<%= f.label :dcode_id,:class=>"input_label" %>
	<div class="input_area">
		<p class="input">
    <div class="reference">
			<%= f.hidden_field :dcode_id,{:class=>"ref_hidden_field"}%>
			<%= text_field_tag "dcode_info",@fee_code_match.dcode.to_s,:class=>"ref" %>
			<a href="#" class-data="U8code"><span class="reference"></span></a>			
		</div>

		</p>
		<p class="tip"></p>
	</div>
	<span class="error_message"></span>    
  </div>
<div class="field">
	<%= f.label :ccode_id,:class=>"input_label" %>
	<div class="input_area">
		<p class="input">
    <div class="reference">
			<%= f.hidden_field :ccode_id,{:class=>"ref_hidden_field"}%>
			<%= text_field_tag "ccode_info",@fee_code_match.ccode.to_s,:class=>"ref" %>
			<a href="#" class-data="U8code"><span class="reference"></span></a>			
		</div>

		</p>
		<p class="tip"></p>
	</div>
	<span class="error_message"></span>    
  </div>
<div class="field">
	<%= f.label :description,:class=>"input_label" %>
	<div class="input_area">
		<p class="input">
			<%= f.text_field :description,:class=>"basic_input" %><%= mark_required(@fee_code_match, :description) %>
		</p>
		<p class="tip"></p>
	</div>
	<span class="error_message"></span>    
  </div>
  <div class="actions">
    <%= f.submit "保存" %>
  </div>
<% end %>

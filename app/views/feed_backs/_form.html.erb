<%= form_for(@feed_back) do |f| %>
<div class="form_title"><h3><%= @title%></h3></div>
<div class="actions">
  <%= link_to "保存",{:href=>"#"},:class=>"submit button2" %>
</div>
<div class="field">
	<%= f.label "意见",:class=>"input_label" %>
	<div class="input_area">
		<p class="input">
			<%= f.text_area :content,:class=>"basic_input" %><%= mark_required(@feed_back, :content) %>
		</p>
		<p class="tip"></p>
	</div>
	<span class="error_message"></span>    
  </div>

<% end %>
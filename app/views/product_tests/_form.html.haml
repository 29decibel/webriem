= form_for [@vrv_project,@product_test] do |f|
  -f.object.class.column_names.reject{|cn|%w(id created_at updated_at).include? cn}.each do |cn|
    =tb_input_field(f,cn)
  .actions
    = f.submit '保存',:class=>'btn primary'
    =link_to "放弃", vrv_project_product_tests_path(@vrv_project),:class=>'btn'

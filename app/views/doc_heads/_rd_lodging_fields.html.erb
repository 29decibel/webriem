<tr class="fields">
	<td>
		<%= f.text_field :sequence,:class=>"small table_row_sequence" %>  
	</td>
	<td>
		<%= f.text_field :days,:class=>"normal" %>  
	</td>
	<td>
		<div class="reference">
			<%= f.hidden_field :region_id,{:class=>"ref_hidden_field"}%>
			<%= text_field_tag "region_info",f.object.region.to_s,:class=>"ref get_fee" %>
			<a href="#" class-data="Region"><span class="reference"></span></a>			
		</div>
	</td>
	<td>
		<%= f.text_field :lodging_date,:class=>"datepicker normal" %>  
	</td>
	<td>
		<%= text_field_tag "fee_standard","暂无",:class=>"normal fee_standard" %>
	</td>
	<td>
		<%= f.text_field :people_count,:class=>"normal" %>  
	</td>
	<td>
		<%= f.text_field :person_names,:class=>"normal" %>  
	</td>
	<td>
		<%= f.text_field :apply_amount,:class=>"normal" %>  
	</td>
	<td>
		<%= f.text_field :hr_amount,:class=>"normal" %>  
	</td>
	<td>
		<%= f.text_field :fi_amount,:class=>"normal" %>  
	</td>
	<td>
		<%= f.hidden_field(:_destroy) %>
		<%= link_to_remove "删除", f %> 
	</td>
</tr>
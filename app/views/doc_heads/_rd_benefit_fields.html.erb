<tr class="fields">
	<td>
		<%= f.text_field :sequence,:class=>"small table_row_sequence" %>  
	</td>
	<td>
		<%= f.text_field :reim_date,:class=>"datepicker normal" %>  
	</td>
	<td>
		<%= f.text_field :people_count,:class=>"normal" %>  
	</td>
	<td>
		<%= f.text_field :fee_time_span,:class=>"normal" %>  
	</td>
	<td>
		<%= f.text_field :amount,:class=>"normal doc_ori_amount" %>  
	</td>
	<td>
		<%= f.text_field :hr_amount,:class=>"doc_HR_amount normal number" %>  
	</td>
	<td>
		<%= f.text_field :fi_amount,:class=>"doc_FI_amount normal number" %>  
	</td>
	<td>
		<%= f.label :uploads,:class=>"normal" %>
		<%= f.hidden_field :id%>
		<%= link_to_function("添加附件","add_upload_files(this,'#{upload_files_path}')")%>
	</td>	
	<td>
		<%= f.hidden_field(:_destroy) %>
		<%= link_to_remove "删除", f %> 
	</td>
</tr>
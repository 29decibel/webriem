<h3>福利费用报销</h3>
<div class="fields">
	<p>  
		<%= f.label :sequence, "序号" %><br />  
		<%= f.text_field :sequence %>  
	</p>
	<p>  
		<%= f.label :reim_date, "报销日期" %><br />  
		<%= f.text_field :reim_date,:class=>"datepicker" %>  
	</p>
	<p>  
		<%= f.label :people_count, "人数" %><br />  
		<%= f.text_field :people_count %>  
	</p>
	<p>  
		<%= f.label :fee_time_span, "费用期间" %><br />  
		<%= f.text_field :fee_time_span %>  
	</p>
	<p>  
		<%= f.label :amount, "申请费用" %><br />  
		<%= f.text_field :amount %>  
	</p>
	<p>
		<%= f.hidden_field(:_destroy) %>
		<%= link_to_remove "删除福利费用报销", f %> 
	</p>
</div>